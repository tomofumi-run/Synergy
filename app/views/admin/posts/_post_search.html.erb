<%= form_with url: search_admin_posts_path, local: true, method: :get do |f|%>
<div class="d-flex">
  <%= f.text_field :content, placeholder: "投稿を検索する", class: 'col-sm-6 form-control' %>
  <%= f.button :type => "submit", class:"btn btn-outline-secondary" do%>
    <i class="fas fa-search"></i>
  <% end %>  
</div>
<% end %>